package com.company;

import java.util.Arrays;

class Main {
     public static void main(String[] args) {
          int arr[]={1,2,4,-1,-5,-6,7,6,4};
          Arrays.sort(arr);
          for(int i=0; i< arr.length;i++){
               System.out.print(" " + arr[i]);
     }
          for(int j= arr.length-1; j>=0; j--){
               System.out.print(" " + arr[j]);
          }
     }
}
